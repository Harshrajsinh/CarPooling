<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cbe.android" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >

        <activity
            android:name="com.android.cbe.ui.dashboard.CBEDashboard"
            android:label="@string/app_name"
            android:theme="@style/Theme.Cbe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.cbe.ui.dashboard.navigationdrawer.common.Feedback"
            android:label="FeedBack"
            android:theme="@style/Theme.Cbe">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.android.cbe.baseapp.baseclass.Activity1"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name="com.android.cbe.baseapp.baseclass.Activity2"
            android:theme="@style/AppTheme" >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.cbe.MainActivity"
            android:theme="@style/AppTheme" >
        </activity>
<activity android:name="com.android.cbe.baseapp.baseclass.HomeActivity">
    <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <category android:name="android.intent.category.DEFAULT" />
    </intent-filter>
</activity>

    </application>

</manifest>





